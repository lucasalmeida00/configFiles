# This is Git's per-user configuration file.
[user]
	name = Lucas Almeida
	email = lucasalmeidasilva0@gmail.com
[alias]
  co = checkout
  lg = log --all --graph --decorate --oneline --abbrev-commit
  cm = commit
  ac = !git add -A && git commit
  st = status -sb
  tags = tag -l
  branches = branch -a
  remotes = remote -v
[push]
	autoSetupRemote = true
